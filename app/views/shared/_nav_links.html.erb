<div class="col-md-10 col-md-offset-1 col-sm-12" id="profile-tabs">
  <div class="row">
    <div class="col-md-10 col-md-offset-2 col-xs-12" >
      <div class="row">
        <div class="col-sm-2 col-sm-offset-1" id="single-profile-tab">
          <%= link_to "Timeline", user_timeline_path(real_user_id), class: "h6" %>
        </div>
        <div class="col-sm-2" id="single-profile-tab-current">
          <%= link_to "About", user_path(real_user_id), class: "h6" %>
        </div>
        <div class="col-sm-2" id="single-profile-tab">
          <%= link_to "Photos (123)", photos_path, class: "h6" %>
        </div>
        <div class="col-sm-2" id="single-profile-tab">
          <%= link_to "Friends (542)", friends_path, class: "h6" %>
        </div>
        <% if real_user_id == current_user.id.to_s %>
        <div class="col-sm-3" id="edit-profile-tab">
          <%= link_to "Edit Profile", edit_user_path(real_user_id), class: "h6" %>
        <% end %>
        </div>
      </div>
    </div>
  </div>
</div>  